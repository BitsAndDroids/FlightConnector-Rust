{
  "identifier": "main",
  "$schema": "../gen/schemas/desktop-schema.json",
  "local": true,
  "windows": ["bits-and-droids-connector"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-start-dragging",
    "window:allow-close",
    "window:allow-minimize",
    "window:allow-toggle-maximize",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "store:allow-keys",
    "store:default",
    "log:default"
  ]
}
